{
  "expo": {
    "name": "TraysikelKO",
    "slug": "traysikelko",
    "version": "1.0.0",
    "icon": "./assets/icon.png",
    "extra": {
      "eas": {
        "projectId": "4fa8f855-66cc-437e-a9f8-ccb4bc9773a6"
      }
    },
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/traysi.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFF8F5"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.traysikelko.app",
      "icon": "./assets/icon.png",
      "infoPlist": {
        "NSPhoneCallUsageDescription": "This app needs access to make emergency phone calls.",
        "NSCameraUsageDescription": "This app needs access to your camera to take photos for your profile and documents.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to select photos for your profile and documents."
      }
    },
    "android": {
      "icon": "./assets/icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#2F80ED"
      },
      "package": "com.traysikelko.app",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "CALL_PHONE",
        "READ_PHONE_STATE",
        "POST_NOTIFICATIONS"
      ]
    },
    "web": {},
    "plugins": [
      "expo-font"
    ]
  }
}
